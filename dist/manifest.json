{
  "manifest_version": 2,
  "name": "Crypto Flux",
  "version": "1.0.0",
  "description": "A crypto price tracker",
  "background": {
    "scripts": [
      "assets/index-306ef382.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "assets/all_sizes.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "https://api.coincap.io/"
  ],
  "browser_action": {
    "default_title": "Crypto Flux",
    "default_popup": "pages/popup/index.html",
    "default_icon": "assets/all_sizes.png"
  }
}